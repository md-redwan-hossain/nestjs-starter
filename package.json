{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "description": "",
  "author": "Md. Redwan Hossain",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "make-env": "node scaffold-env-file.cjs",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:staging": "NODE_ENV=development pm2 start dist/main.js --name 'nest staging' --max-memory-restart 1000M --attach",
    "start:prod": "NODE_ENV=production pm2 start dist/main.js --name 'nest prod' --max-memory-restart 1000M --attach",
    "down:staging": "pm2 stop 'nest staging'",
    "down:prod": "pm2 stop 'nest prod'",
    "apply-changes": "git pull && npm i &&npm audit fix && npm run build && pm2 reload all --update-env",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrator:new": "npx dbmate --env-file '.env.migrator' new",
    "migrator:apply": "npx dbmate --env-file '.env.migrator' migrate --strict",
    "migrator:rollback": "npx dbmate  --env-file '.env.migrator' rollback",
    "migrator:list": "npx dbmate --env-file '.env.migrator' status",
    "migrator:schema-force-push": "npx dbmate --env-file '.env.migrator' load"
  },
  "dependencies": {
    "@bull-board/api": "^5.14.0",
    "@bull-board/express": "^5.14.0",
    "@bull-board/nestjs": "^5.14.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/cache-manager": "^2.2.0",
    "@nestjs/common": "^10.3.1",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.1",
    "@nestjs/event-emitter": "^2.0.3",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.1",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/terminus": "^10.2.1",
    "@nestjs/throttler": "^5.1.1",
    "@theinternetfolks/snowflake": "^1.3.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^4.14.4",
    "cache-manager": "^5.4.0",
    "cache-manager-ioredis-yet": "^1.2.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "crypto-js": "^4.2.0",
    "dateformat": "^5.0.3",
    "dayjs": "^1.11.10",
    "drizzle-orm": "^0.29.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
