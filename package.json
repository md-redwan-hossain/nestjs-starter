    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.1",
    "@swc/cli": "^0.3.10",
    "@swc/core": "^1.3.107",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/crypto-js": "^4.2.2",
    "@types/dateformat": "^5.0.2",
    "@types/express": "^4.17.21",
